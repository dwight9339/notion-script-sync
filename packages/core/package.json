{
  "name": "@sprongus/core",
  "version": "1.0.0",
  "description": "This project streamlines the process of synchronizing a granular YouTube video script (written as a nested document) with a Notion storyboard database.  \r It’s designed for creators who want creative freedom in drafting scripts—while maintaining precise, up-to-date alignment with their production visuals and assets.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwight9339/sprongus.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dwight9339/sprongus/issues"
  },
  "homepage": "https://github.com/dwight9339/sprongus#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@notionhq/client": "^3.1.3",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.2.0",
    "csv-parse": "^5.6.0",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.5.0",
    "env-paths": "^3.0.0",
    "zod": "^3.25.74"
  }
}
